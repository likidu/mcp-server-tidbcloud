{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd ../.. && pnpm install && pnpm build",
  "framework": null,
  "rewrites": [
    {
      "source": "/mcp/:path*",
      "destination": "/api/mcp"
    },
    {
      "source": "/mcp",
      "destination": "/api/mcp"
    },
    {
      "source": "/:path*",
      "destination": "/api?path=:path*"
    }
  ]
}
